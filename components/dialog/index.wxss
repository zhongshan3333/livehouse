.dialog_box {
  width: 540rpx;
  height: 324rpx;
  border-radius: 16rpx;
  overflow: hidden;
  position: fixed;
  top: 50%;
  left: 0;
  z-index: 1001;
  background: #fafafa;
  margin: -150rpx 12% 0 12%;

}

.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: 999;
  left: 0;
  top: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;

}

.cancel {
  width: 100%;
  height: 100%;
  text-align: center;
  font-size: 34rpx;
  color: #0A0A17;
}

.Sure {
  width: 100%;
  height: 100%;
  font-size: 34rpx;
  color: #FC6F43;
  border-left: 1rpx solid #dcdcdc;
  text-align: center;
}

.title {
  font-size: 36rpx;
  color: #0A0A17;
  padding: 32rpx;
  text-align: center;
}

.content {
  overflow-y: scroll;
  width: 80%;
  height: 70rpx;
  font-size: 28rpx;
  color: #7D7D7D;
  margin-left: 50rpx;
  /*超出父盒子高度可滚动*/
}

.input_show1 {
  margin: 0 auto;
  width: 80%;
  margin-left: 10%;
  font-size: 32rpx;
  text-align: center;
}

.btn1 {
  width: 100%;
  height: 100rpx;
  display: flex;
  border-top: 1rpx solid #DCDCDC;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  margin-top: 45rpx;
}

.modalMsg {
  text-align: center;
  margin-top: 45rpx;
  display: block;
}
.input_content{
  width:500rpx;
  height:70rpx;
  margin-left: 10rpx;

}
.nameInput{
  padding:15rpx;
  border-bottom: 1rpx solid #999;
}